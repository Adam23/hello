<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>	
  	<servlet>
		<servlet-name>s1</servlet-name>
		<servlet-class>com.servlet.hello</servlet-class>
	</servlet>
  <servlet>
    <servlet-name>one</servlet-name>
    <servlet-class>com.servlet.one</servlet-class>
	
  </servlet>
  <servlet>
    <servlet-name>two</servlet-name>
    <servlet-class>com.servlet.two</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>downLoad</servlet-name>
    <servlet-class>com.servlet.downLoad</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>getProperties</servlet-name>
    <servlet-class>com.servlet.getProperties</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Login1</servlet-name>
    <servlet-class>com.servlet.Login</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Login2</servlet-name>
    <servlet-class>com.servlet.Login2</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Register</servlet-name>
    <servlet-class>com.scg.web.ui.Register</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RegServlet</servlet-name>
    <servlet-class>com.scg.web.controlor.RegServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>com.scg.web.ui.Login</servlet-class>
  </servlet>









	
	<servlet-mapping>
		<servlet-name>s1</servlet-name>
		<url-pattern>/hi</url-pattern>
	</servlet-mapping>
  <servlet-mapping>
    <servlet-name>one</servlet-name>
    <url-pattern>/one</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>two</servlet-name>
    <url-pattern>/two</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>downLoad</servlet-name>
    <url-pattern>/downLoad</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>getProperties</servlet-name>
    <url-pattern>/servlet/getProperties</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Login1</servlet-name>
    <url-pattern>/Login1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Login2</servlet-name>
    <url-pattern>/Login2</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Register</servlet-name>
    <url-pattern>/reg</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegServlet</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/Login</url-pattern>
  </servlet-mapping>
  <error-page>
	  	<exception-type>java.lang.Throwable</exception-type>
	  	<location>/error.jsp</location>
   </error-page>
  <error-page>
  	<error-code>404</error-code>
  	<location>/404.jsp</location>
  </error-page>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
